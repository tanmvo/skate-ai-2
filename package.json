{
  "name": "skate-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:init": "npx tsx scripts/init-db.ts",
    "db:reset": "prisma db push --force-reset && npm run db:init && npm run dev:clean-files --yes",
    "dev:clean-files": "npx tsx scripts/cleanup-dev-files.ts --files",
    "dev:clean-db": "npx tsx scripts/cleanup-dev-files.ts --database",
    "dev:clean-all": "npx tsx scripts/cleanup-dev-files.ts --all",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "eval:export": "npx tsx scripts/export-for-evaluation.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/react": "^2.0.9",
    "@anthropic-ai/sdk": "^0.57.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@vercel/blob": "^1.1.1",
    "ai": "^5.0.9",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "form-data": "^4.0.4",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.526.0",
    "mammoth": "^1.9.1",
    "next": "15.4.4",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "pdf2json": "^3.2.0",
    "posthog-js": "^1.260.1",
    "posthog-node": "^5.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^9.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "voyageai": "^0.0.5",
    "zod": "^3.25.76"
